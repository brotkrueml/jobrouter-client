{
    "name": "brotkrueml/jobrouter-client",
    "description": "Rest client for the JobRouter® digitisation platform",
    "type": "library",
    "keywords": [
        "jobrouter",
        "rest client"
    ],
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Chris Müller",
            "role": "Developer"
        }
    ],
    "support": {
        "docs": "https://jobrouter-client.readthedocs.io/",
        "issues": "https://github.com/brotkrueml/jobrouter-client/issues",
        "source": "https://github.com/brotkrueml/jobrouter-client"
    },
    "require": {
        "php": ">=7.3",
        "ext-curl": "*",
        "ext-json": "*",
        "kriswallsmith/buzz": "^1.2",
        "nyholm/psr7": "^1.3",
        "symfony/polyfill-php80": "^1.18"
    },
    "require-dev": {
        "captainhook/captainhook": "^5.4",
        "donatj/mock-webserver": "^2.1",
        "friendsofphp/php-cs-fixer": "^2.16",
        "infection/infection": ">=0.18 <=0.19",
        "mikey179/vfsstream": "^1.6",
        "php-coveralls/php-coveralls": "^2.2",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.8",
        "vimeo/psalm": "^4.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Brotkrueml\\JobRouterClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brotkrueml\\JobRouterClient\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.0-dev"
        }
    },
    "scripts": {
        "fix-cs": "vendor/bin/php-cs-fixer fix"
    }
}
